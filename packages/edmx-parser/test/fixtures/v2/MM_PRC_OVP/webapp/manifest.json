                "preload": true,
                "dataSource": "MM_PRC_OVP",
                "settings": {
                    "defaultCountMode": "None"
                }
            },
            "MM_PRC_OVP2": {
                "preload": true,
                "dataSource": "MM_PRC_OVP",
                "settings": {
                    "defaultCountMode": "None"
                }
            },
            "SupplierEvaluationOverall": {
                "preload": true,
                "dataSource": "SupplierEvalOverall",
                "settings": {
                    "defaultCountMode": "None"
                }
            },
            "C_NONMNGDPURGSPEND_CDS": {
                "preload": true,
                "dataSource": "C_NONMNGDPURGSPEND_CDS",
                "settings": {
                    "defaultCountMode": "Inline"
                }
            },
            "OFFCONTRACTSPEND": {
                "preload": true,
                "dataSource": "OFFCONTRACTSPEND",
                "settings": {
                    "defaultCountMode": "Inline"
                }
            },
            "REQUISITIONNOTOUCH": {
                "preload": true,
                "dataSource": "REQUISITIONNOTOUCH",
                "settings": {
                    "defaultCountMode": "Inline"
                }
            },
