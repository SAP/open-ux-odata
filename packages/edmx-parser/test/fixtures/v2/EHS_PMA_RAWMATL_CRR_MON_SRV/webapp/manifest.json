                    "lazy": true
                },
                "sap.ehs.fnd.lib.reuse.pc": {
                    "lazy": true
                }
            },
            "components": {}
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "reusepc_i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
