            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": ["sap_hcb", "sap_belize"]
    },
    "sap.ui5": {
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "${sap.ui5.dist.version}",
            "libs": {
                "sap.ui.core": {},
                "sap.ui.generic.app": {},
                "sap.suite.ui.generic.template": {},
                "sap.collaboration": {
                    "lazy": true
                },
                "sap.f": {
                    "lazy": true
