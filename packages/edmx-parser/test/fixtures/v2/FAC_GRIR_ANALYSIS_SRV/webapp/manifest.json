{
    "_version": "1.5.0",

    "sap.app": {
        "id": "fin.fc.grir.overview",
        "type": "application",
        "i18n": "i18n/i18n.properties",

        "applicationVersion": {
            "version": "${project.version}"
        },

        "title": "{{app_title}}",
        "description": "{{app_description}}",
        "ach": "FI-FIO-GL",
        "resources": "resources.json",

        "dataSources": {
            "FAC_GRIR_ANALYSIS_SRV": {
                "uri": "/sap/opu/odata/sap/FAC_GRIR_ANALYSIS_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": ["FAC_GRIR_ANALYSIS_ANNO_MDL", "frontendAnnotations"],
                    "localUri": "localService/FAC_GRIR_ANALYSIS_SRV/metadata.xml"
                }
            },

            "FAC_GRIR_ANALYSIS_ANNO_MDL": {
                "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='FAC_GRIR_ANALYSIS_ANNO_MDL',Version='0001')/$value/",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "localService/FAC_GRIR_ANALYSIS_SRV/FAC_GRIR_ANALYSIS_ANNO_MDL.xml"
                }
            },

            "frontendAnnotations": {
                "type": "ODataAnnotation",
                "uri": "annotations/frontendAnnotations.xml",
                "settings": {
                    "localUri": "annotations/frontendAnnotations.xml"
                }
            }
        },

        "sourceTemplate": {
            "id": "OVP.smartovptemplate",
            "version": "1.41.1"
        }
    },

    "sap.ui": {
        "technology": "UI5",

        "icons": {
            "icon": ""
        },

        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },

        "supportedThemes": ["sap_hcb", "sap_belize"]
    },

    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "${sap.ui5.dist.version}",
            "libs": {
                "sap.ovp": { "lazy": false }
            }
        },

        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "analyticalPOIinformation": {
                "preload": false,
                "dataSource": "FAC_GRIR_ANALYSIS_SRV",
                "settings": {
                    "defaultCountMode": "None"
                }
            },
            "analyticalPOIsurplusInformation": {
                "preload": false,
                "dataSource": "FAC_GRIR_ANALYSIS_SRV",
                "settings": {
                    "defaultCountMode": "None"
                }
            },
            "analyticalFIOIinformation": {
                "preload": false,
                "dataSource": "FAC_GRIR_ANALYSIS_SRV",
                "settings": {
                    "defaultCountMode": "None"
                }
            },
            "listFIOIamounts": {
                "preload": false,
                "dataSource": "FAC_GRIR_ANALYSIS_SRV",
                "settings": {
                    "defaultCountMode": "Inline"
                }
            },
            "analyticalFIOIamounts": {
                "preload": false,
                "dataSource": "FAC_GRIR_ANALYSIS_SRV",
                "settings": {
                    "defaultCountMode": "None"
                }
            }
        },

        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {
                    "sap.ovp.app.Main": {
                        "controllerName": "fin.fc.grir.overview.ext.controller.OVPExtension"
                    }
                }
            }
        },

        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },

    "sap.ovp": {
        "globalFilterModel": "analyticalFIOIamounts",
        "globalFilterEntityType": "C_GRIRProcDigestInGlobalCrcyType",

