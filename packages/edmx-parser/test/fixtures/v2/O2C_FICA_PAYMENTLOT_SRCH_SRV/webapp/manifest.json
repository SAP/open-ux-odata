{
    "_version": "1.8.0",
    "sap.app": {
        "id": "${project.artifactId}",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "${project.version}"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "tags": {
            "keywords": []
        },
        "ach": "FI-CAC",
        "resources": "resources.json",
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/O2C_FICA_PAYMENTLOT_SRCH_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": ["O2C_FICA_PAYMENTLOT_SRC_ANNO_MDL", "localAnnotations"],
                    "localUri": "localService/metadata.xml"
                }
            },
            "O2C_FICA_PAYMENTLOT_SRC_ANNO_MDL": {
                "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='O2C_FICA_PAYMENTLOT_SRC_ANNO_MDL',Version='0001')/$value/",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "localService/O2C_FICA_PAYMENTLOT_SRC_ANNO_MDL.xml"
                }
            },
            "localAnnotations": {
                "uri": "annotations/annotations.xml",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "annotations/annotations.xml"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "searchInPaymentLot": {
                    "semanticObject": "CAPayment",
                    "action": "searchInPaymentLot",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                }
            },
            "outbounds": {
                "PaymentLot": {
                    "semanticObject": "CAPaymentLot",
                    "action": "displayItem"
                },
                "ChequeLot": {
                    "semanticObject": "CAChequeLot",
                    "action": "process"
                },
                "PaymenttOrderLot": {
                    "semanticObject": "CAPaymentOrderLot",
                    "action": "process"
                },
                "CreditCardLot": {
                    "semanticObject": "CACreditCardLot",
                    "action": "process"
                }
            }
        },
        "offline": false,
        "sourceTemplate": {
            "id": "ui5template.smartTemplate",
            "version": "1.40.12"
        }
    },
    "sap.copilot": {
        "_version": "1.0.0",
        "contextAnalysis": {
            "allowAddingObjectsFromAppScreenToCollection": false
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": false
        },
        "supportedThemes": ["sap_hcb", "sap_belize"]
    },
    "sap.ui5": {
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "${sap.ui5.dist.version}",
            "libs": {
                "sap.ui.core": {
                    "lazy": false
                },
                "sap.ui.generic.app": {
                    "lazy": false
                },
                "sap.suite.ui.generic.template": {
                    "lazy": false
                }
            },
            "components": {}
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ListReport|C_CAPaymentLotSearch": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ListReport/C_CAPaymentLotSearch/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ObjectPage|C_CAPaymentLotSearch": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ObjectPage/C_CAPaymentLotSearch/i18n.properties"
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false
                }
            }
        },
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {
                    "sap.suite.ui.generic.template.ListReport.view.ListReport": {
                        "controllerName": "cus.o2c.paymentlot.search.s1.ext.controller.ListReportExt"
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.ui.generic.app": {
        "_version": "1.3.0",
        "settings": {
            "flexibleColumnLayout": {
                "defaultTwoColumnLayoutType": "TwoColumnsMidExpanded",
                "defaultThreeColumnLayoutType": "ThreeColumnsEndExpanded"
            }
        },
        "pages": {
            "ListReport|C_CAPaymentLotSearch": {
                "entitySet": "C_CAPaymentLotSearch",
                "component": {
                    "name": "sap.suite.ui.generic.template.ListReport",
                    "list": true,
                    "settings": {
                        "smartVariantManagement": true
                    }
                },

                "pages": {
                    "ObjectPage|C_CAPaymentLotSearch": {
                        "entitySet": "C_CAPaymentLotSearch",
                        "component": {
                            "name": "sap.suite.ui.generic.template.ObjectPage"
                        }
                    }
                }
            }
        }
    },
    "sap.fiori": {
        "registrationIds": ["F3978"],
        "archeType": "transactional"
    },
    "sap.platform.hcp": {
        "uri": ""
    }
}
