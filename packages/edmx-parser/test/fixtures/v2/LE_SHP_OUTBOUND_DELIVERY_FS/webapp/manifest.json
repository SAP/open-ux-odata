{
    "_version": "1.7.0",
    "sap.app": {
        "id": "${project.artifactId}",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "${project.version}"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "tags": {
            "keywords": []
        },
        "ach": "LE-SHP-FIO-OBP",
        "resources": "resources.json",
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/LE_SHP_OUTBOUND_DELIVERY_FS/",
                "type": "OData",
                "settings": {
                    "annotations": ["LE_SHP_OUTBOUND_DELIVER_ANNO_MDL", "localAnnotations"],
                    "localUri": "localService/metadata.xml"
                }
            },
            "LE_SHP_OUTBOUND_DELIVER_ANNO_MDL": {
                "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='LE_SHP_OUTBOUND_DELIVER_ANNO_MDL',Version='0001')/$value",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "localService/LE_SHP_OUTBOUND_DELIVER_ANNO_MDL.xml"
                }
            },
            "localAnnotations": {
                "uri": "annotations/annotations.xml",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "annotations/annotations.xml"
                }
            }
        },
        "offline": false,
        "sourceTemplate": {
            "id": "ui5template.smarttemplate",
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "_version": "1.2.0",
        "fullWidth": true,

        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": ["sap_hcb", "sap_bluecrystal"]
    },
    "sap.ui5": {
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "${sap.ui5.dist.version}",
            "libs": {
                "sap.ui.core": { "lazy": false },
                "sap.suite.ui.generic.template": { "lazy": false },
                "sap.ui.generic.app": { "lazy": false },
                "sap.m": { "lazy": false },
                "sap.s4.cfnd.lib.businessprocessflow": { "lazy": false },
                "sap.s4h.cfnd.featuretoggle": { "minVersion": "11.7.0-SNAPSHOT", "lazy": false },
                "sap.ssuite.fnd.om.outputcontrol": { "lazy": false }
            },
            "components": {
                "sap.s4.cfnd.lib.businessprocessflow.component.smarttemplate": { "lazy": false },
                "sap.se.mi.plm.lib.attachmentservice.attachment.components.stcomponent": { "lazy": true }
            }
        },
        "componentUsages": {
            "attachmentReuseComponent": {
                "name": "sap.se.mi.plm.lib.attachmentservice.attachment",
                "settings": {}
            }
        },

        "models": {
            "i18n": {
                "preload": false,
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "@i18n": {
                "preload": false,
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ListReport|C_OutboundDeliveryFs": {
                "preload": false,
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ListReport/C_OutboundDeliveryFs/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ObjectPage|C_OutboundDeliveryFs": {
                "preload": false,
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ObjectPage/C_OutboundDeliveryFs/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ObjectPage|C_OutboundDeliveryItemFs": {
                "preload": false,
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ObjectPage/C_OutboundDeliveryItemFs/i18n.properties"
            },
            "": {
                "preload": true,
                "dataSource": "mainService",
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "metadataUrlParams": {
                        "sap-documentation": "heading,quickinfo"
                    }
                }
            }
        },
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {
                    "sap.suite.ui.generic.template.ObjectPage.view.Details": {
                        "controllerName": "i2d.le.delivery.op.obdlv.ext.controllers.ObjectPageExtension",
                        "sap.ui.generic.app": {
                            "C_OutboundDeliveryFs": {
                                "EntitySet": "C_OutboundDeliveryFs"
                            }
                        }
                    }
                },
                "sap.ui.viewExtensions": {
                    "sap.suite.ui.generic.template.ObjectPage.view.Details": {
                        "ReplaceFacet|C_OutboundDeliveryFs|OutputManagement": {
                            "className": "sap.ui.core.mvc.View",
                            "viewName": "i2d.le.delivery.op.obdlv.ext.view.OutputManagement",
                            "type": "XML",
                            "sap.ui.generic.app": {
                                "title": "{{OUTPUT_MANAGEMENT}}"
                            }
                        },
                        "ReplaceFacet|C_OutboundDeliveryItemFs|OutputManagement": {
                            "className": "sap.ui.core.mvc.View",
                            "viewName": "i2d.le.delivery.op.obdlv.ext.view.OutputManagement",
                            "type": "XML",
                            "sap.ui.generic.app": {
                                "title": "{{OUTPUT_MANAGEMENT}}"
                            }
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.ui.generic.app": {
        "_version": "1.3.0",
        "pages": {
            "ObjectPage|C_OutboundDeliveryFs": {
                "entitySet": "C_OutboundDeliveryFs",
                "component": {
                    "name": "sap.suite.ui.generic.template.ListReport",
                    "list": true
                },

                "pages": {
                    "ObjectPage|C_OutboundDeliveryFs": {
                        "entitySet": "C_OutboundDeliveryFs",
                        "component": {
                            "name": "sap.suite.ui.generic.template.ObjectPage",
                            "settings": {
                                "showRelatedApps": true
                            }
                        },
                        "embeddedComponents": {
                            "simple::processflow": {
                                "componentName": "sap.s4.cfnd.lib.businessprocessflow.component.smarttemplate",
                                "id": "simple::processflow",
                                "propogateModel": true,
                                "title": "{{processFlow}}",
                                "settings": {
                                    "layoutID": "C_OutbDelivProcFlow",
                                    "docID": "{parts:[{path:'OutboundDelivery'}]}",
                                    "keyName": "OutboundDelivery"
                                }
                            },
                            "simple::Attachments": {
                                "componentName": "sap.se.mi.plm.lib.attachmentservice.attachment.components.stcomponent",
                                "id": "simple::Attachments",
                                "title": "{{AttachmentsReuseFacetLabel}}",
                                "settings": {
                                    "mode": "D",
                                    "objectType": "LIKP",
                                    "objectKey": "{parts:[{path:'OutboundDelivery'}], formatter:'i2d.le.delivery.op.obdlv.model.formatter.returnAttachmentKey'}",
                                    "attributeHandling": {
                                        "_VisibleAttributes": {
                                            "UPLOADEDBY": true,
                                            "UPLOADEDON": true,
                                            "FILESIZE": true,
                                            "ENABLELINK": true,
                                            "ATTACHMENTSTATUS": true,
                                            "ATTACHMENTTITLE": true,
                                            "SOURCE": true,
                                            "DIRDETAILS": false
                                        },
                                        "_VisibleActions": {
                                            "RENAME": false,
                                            "DELETE": false,
                                            "ADD": false,
                                            "ADDURL": false,
                                            "DOWNLOAD": false
                                        }
                                    }
                                }
                            }
                        },
                        "pages": {
                            "ObjectPage|to_OutboundDeliveryitemFs": {
                                "navigationProperty": "to_OutboundDeliveryItemFs",
                                "entitySet": "C_OutboundDeliveryItemFs",
                                "component": {
                                    "name": "sap.suite.ui.generic.template.ObjectPage"
                                },
                                "pages": {
                                    "ObjectPage|to_SalesOrder": {
                                        "navigationProperty": "to_SalesOrder",
                                        "entitySet": "C_RelSalesOrderItmDlvrFSS",
                                        "component": {
                                            "name": "sap.suite.ui.generic.template.ObjectPage"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "sap.platform.abap": {
        "uri": "pathabap"
    },
    "sap.fiori": {
        "registrationIds": ["F0233A"],
        "archeType": "factsheet"
    },
    "sap.platform.hcp": {
        "uri": ""
    }
}
