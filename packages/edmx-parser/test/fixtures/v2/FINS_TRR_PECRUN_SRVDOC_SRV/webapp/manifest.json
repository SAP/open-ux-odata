{
    "_version": "1.7.0",
    "sap.app": {
        "_version": "1.2.0",
        "id": "${project.artifactId}",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "${project.version}"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "tags": {
            "keywords": []
        },
        "ach": "CO-PC-OBJ-EBR",
        "resources": "resources.json",
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/FINS_TRR_PECRUN_SRVDOC_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": ["FINS_TRR_PECRUN_SRVDOC_ANNO_MDL", "localAnnotations"],
                    "localUri": "localService/metadata.xml"
                }
            },
            "FINS_TRR_PECRUN_SRVDOC_ANNO_MDL": {
                "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='FINS_TRR_PECRUN_SRVDOC_ANNO_MDL',Version='0001')/$value/",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "localService/FINS_TRR_PECRUN_SRVDOC_ANNO_MDL.xml"
                }
            },
            "localAnnotations": {
                "uri": "annotations/annotations.xml",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "annotations/annotations.xml"
                }
            }
        },
        "offline": false,
        "sourceTemplate": {
            "id": "ui5template.smartTemplate",
            "version": "1.40.12"
        },
        "crossNavigation": {
            "inbounds": {},
            "outbounds": {
                "realTimeIssues": {
                    "semanticObject": "EventBasedRevnRecgn",
                    "action": "manageRevRecRealTimeIssues"
                },
                "backgroundJobs": {
                    "semanticObject": "FinanceApplicationJob",
                    "action": "scheduleRevRecServiceDocuments"
                }
            }
        }
    },
    "sap.ui": {
        "_version": "1.2.0",
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": ["sap_hcb", "sap_belize"]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "${sap.ui5.dist.version}",
            "libs": {
                "sap.ui.core": {
                    "lazy": false
                },
                "sap.m": {
                    "lazy": false
                },
                "sap.suite.ui.generic.template": {
                    "lazy": false
                },
                "sap.ui.generic.app": {
                    "lazy": false
                },
                "sap.nw.core.applogs.lib.reuse": {
                    "lazy": true
                }
            },
            "components": {}
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ListReport|C_EvtBsdRevnRecSrvcDocIssueSet": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ListReport/C_EvtBsdRevnRecSrvcDocIssueSet/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ObjectPage|C_EvtBsdRevnRecSrvcDocIssueSet": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ObjectPage/C_EvtBsdRevnRecSrvcDocIssueSet/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ObjectPage|C_EvtBsdRevnRecSrvcDocIssue": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ObjectPage/C_EvtBsdRevnRecSrvcDocIssue/i18n.properties"
            },
            "": {
                "preload": true,
                "dataSource": "mainService",
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false
                }
            }
        },
        "extends": {
            "extensions": {
                "minVersion": "1.1.0",
                "sap.ui.viewExtensions": {
                    "sap.suite.ui.generic.template.ListReport.view.ListReport": {
                        "ResponsiveTableColumnsExtension|C_EvtBsdRevnRecSrvcDocIssueSet": {
                            "className": "sap.ui.core.Fragment",
                            "fragmentName": "fin.acc.manageRevRecServiceDocuments.ext.fragments.StatusColumnExtension",
                            "type": "XML"
                        },
                        "ResponsiveTableCellsExtension|C_EvtBsdRevnRecSrvcDocIssueSet": {
                            "className": "sap.ui.core.Fragment",
                            "fragmentName": "fin.acc.manageRevRecServiceDocuments.ext.fragments.StatusCellExtension",
                            "type": "XML"
                        }
                    }
                },
                "sap.ui.controllerExtensions": {
                    "sap.suite.ui.generic.template.ListReport.view.ListReport": {
                        "controllerName": "fin.acc.manageRevRecServiceDocuments.ext.controller.ListReportExt",
                        "sap.ui.generic.app": {
                            "C_EvtBsdRevnRecSrvcDocIssueSet": {
                                "EntitySet": "C_EvtBsdRevnRecSrvcDocIssueSet",
                                "Actions": {
                                    "simulateBtn": {
                                        "id": "simulateBtn",
                                        "text": "{i18n>SimulateButton}",
                                        "press": "onSimulateClick"
                                    },
                                    "reprocessBtn": {
                                        "id": "reprocessBtn",
                                        "text": "{i18n>ReprocessButton}",
                                        "press": "onReprocessClick"
                                    },
                                    "deleteBtn": {
                                        "id": "deleteBtn",
                                        "text": "{i18n>DeleteButton}",
                                        "press": "onDeleteClick"
                                    },
                                    "realTimeIssuesNav": {
                                        "id": "realTimeIssuesNavBtn",
                                        "text": "{i18n>RealTimeIssues}",
                                        "press": "onRealTimeIssuesNavClick",
                                        "global": true
                                    },
                                    "backgroundJobsNav": {
                                        "id": "backgroundJobsNavBtn",
                                        "text": "{i18n>BackgroundJobs}",
                                        "press": "onBackgroundJobsNavClick",
                                        "global": true
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.ui.generic.app": {
        "_version": "1.3.0",
        "settings": {
            "flexibleColumnLayout": {
                "defaultTwoColumnLayoutType": "TwoColumnsMidExpanded",
                "defaultThreeColumnLayoutType": "ThreeColumnsEndExpanded",
                "maxColumnsCount": 2
            },
            "forceGlobalRefresh": false
        },
        "pages": {
            "ListReport|C_EvtBsdRevnRecSrvcDocIssueSet": {
                "entitySet": "C_EvtBsdRevnRecSrvcDocIssueSet",
                "component": {
                    "name": "sap.suite.ui.generic.template.ListReport",
                    "list": true,
                    "settings": {
                        "smartVariantManagement": true
                    }
                },
                "pages": {
                    "ObjectPage|to_ServiceDocumentIssDetail": {
                        "navigationProperty": "to_ServiceDocumentIssDetail",
                        "entitySet": "C_EvtBsdRevnRecgnSrvcDocIssDet",
                        "component": {
                            "name": "sap.suite.ui.generic.template.ObjectPage"
                        }
                    },
                    "ObjectPage|to_resultPage": {
                        "defaultLayoutType": "OneColumn",
                        "routingSpec": {
                            "noOData": true,
                            "routeName": "to_resultPage",
                            "noKey": false
                        },
                        "component": {
                            "name": "sap.suite.ui.generic.template.Canvas",
                            "settings": {
                                "requiredControls": {
                                    "footerBar": false,
                                    "paginatorButtons": false,
                                    "tableType": "AnalyticalTable"
                                }
                            }
                        },
                        "implementingComponent": {
                            "componentName": "fin.acc.manageRevRecServiceDocuments.component.closingRunResults",
                            "settings": {}
                        }
                    }
                }
            }
        }
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": ["F4186"],
        "archeType": "transactional"
    },
    "sap.platform.hcp": {
        "_version": "1.2.0",
        "uri": ""
    }
}
