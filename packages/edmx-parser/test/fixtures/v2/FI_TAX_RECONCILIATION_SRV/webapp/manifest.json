{
    "_version": "1.7.0",
    "sap.app": {
        "_version": "1.2.0",
        "id": "${project.artifactId}",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "${project.version}"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "tags": {
            "keywords": []
        },
        "ach": "FI-FIO-GL",
        "resources": "resources.json",
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/FI_TAX_RECONCILIATION_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": ["FI_TAX_RECONCILIATION_ANNO_MDL", "localAnnotations"],
                    "localUri": "localService/metadata.xml",
                    "maxAge": 7200
                }
            },
            "FI_TAX_RECONCILIATION_ANNO_MDL": {
                "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='FI_TAX_RECONCILIATION_ANNO_MDL',Version='0001')/$value/",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "localService/FI_TAX_RECONCILIATION_ANNO_MDL.xml"
                }
            },
            "localAnnotations": {
                "uri": "annotations/annotations.xml",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "annotations/annotations.xml"
                }
            }
        },
        "offline": false,
        "sourceTemplate": {
            "id": "ui5template.smarttemplate",
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "_version": "1.2.0",
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": ["sap_hcb", "sap_bluecrystal"]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "${sap.ui5.dist.version}",
            "libs": {
                "sap.ui.core": { "lazy": true },
                "sap.m": { "lazy": false },
                "sap.ui.comp": { "lazy": false },
                "sap.uxap": { "lazy": false },
                "sap.ui.generic.app": {},
                "sap.suite.ui.generic.template": {}
            },
            "components": {}
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ListReport|C_TaxReconcilAccountQry": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ListReport/C_TaxReconcilAccountQry/i18n.properties"
            },
            "": {
                "preload": true,
                "dataSource": "mainService",
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "None",
                    "refreshAfterChange": false
                }
            }
        },
        "extends": {
            "extensions": {
                "sap.ui.viewExtensions": {
                    "sap.suite.ui.generic.template.ListReport.view.ListReport": {
                        "SmartFilterBarControlConfigurationExtension|C_TaxReconcilAccountQry": {
                            "className": "sap.ui.core.Fragment",
                            "fragmentName": "fin.gl.tax_reconciliationaccountbalance.ext.fragment.customfilter",
                            "type": "XML"
                        }
                    }
                },
                "sap.ui.controllerExtensions": {
                    "sap.suite.ui.generic.template.ListReport.view.ListReport": {
                        "controllerName": "fin.gl.tax_reconciliationaccountbalance.ext.controller.customfilter"
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.ui.generic.app": {
        "_version": "1.1.0",
        "pages": [
            {
                "entitySet": "C_TaxReconcilAccountQry",
                "component": {
                    "name": "sap.suite.ui.generic.template.ListReport",
                    "list": true,
                    "settings": {
                        "condensedTableLayout": true
                    }
                }
            }
        ]
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": ["F2095"],
        "archeType": "transactional"
    },
    "sap.platform.hcp": {
        "_version": "1.2.0",
        "uri": ""
    }
}
