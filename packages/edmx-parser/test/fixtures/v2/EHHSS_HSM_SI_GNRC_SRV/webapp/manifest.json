{
    "_version": "1.8.0",
    "sap.app": {
        "id": "${project.artifactId}",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "${project.version}"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "tags": {
            "keywords": []
        },
        "ach": "EHS-SUS-HS",
        "resources": "resources.json",
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/EHHSS_HSM_SI_GNRC_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": ["EHHSS_HSM_SI_GNRC_ANNO_MDL", "localAnnotations"],
                    "localUri": "localService/metadata.xml"
                }
            },
            "replica": {
                "uri": "/sap/opu/odata/sap/EHHSS_HSM_SI_GNRC_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": ["replicaLocalAnnotations"],
                    "localUri": "localService/metadata.xml"
                }
            },
            "EHHSS_HSM_SI_GNRC_ANNO_MDL": {
                "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='EHHSS_HSM_SI_GNRC_ANNO_MDL',Version='0001')/$value/",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "localService/EHHSS_HSM_SI_GNRC_ANNO_MDL.xml"
                }
            },
            "localAnnotations": {
                "uri": "annotations/annotations.xml",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "annotations/annotations.xml"
                }
            },
            "replicaLocalAnnotations": {
                "uri": "localService/EHHSS_HSM_SI_GNRC_ANNO_MDL.xml",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "localService/EHHSS_HSM_SI_GNRC_ANNO_MDL.xml"
                }
            }
        },
        "offline": false,
        "sourceTemplate": {
            "id": "ui5template.smartTemplate",
            "version": "1.40.12"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": ["sap_hcb", "sap_belize"]
    },
    "sap.ui5": {
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "${sap.ui5.dist.version}",
            "libs": {
                "sap.ui.generic.app": {},
                "sap.suite.ui.generic.template": {},
                "sap.m": {
                    "lazy": false
                },
                "sap.ui.comp": {
                    "lazy": false
                },
                "sap.ui.richtexteditor": {
                    "lazy": false
                },
                "sap.ui.layout": {
                    "lazy": false
                },
                "sap.ui.fl": {
                    "lazy": false
                }
            },
            "components": {}
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ListReport|C_GenericSafetyInstructionTP": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ListReport/C_GenericSafetyInstructionTP/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ObjectPage|C_GenericSafetyInstructionTP": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ObjectPage/C_GenericSafetyInstructionTP/i18n.properties"
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "metadataUrlParams": {
                        "sap-value-list": "none"
                    }
                }
            },
            "replica": {
                "dataSource": "replica",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "metadataUrlParams": {
                        "sap-value-list": "none"
                    }
                }
            }
        },
        "extends": {
            "extensions": {
                "sap.ui.viewExtensions": {
                    "sap.suite.ui.generic.template.ListReport.view.ListReport": {
                        "ResponsiveTableColumnsExtension|C_GenericSafetyInstructionTP": {
                            "type": "XML",
                            "className": "sap.ui.core.Fragment",
                            "fragmentName": "ehs.hs.si.managegnrcs1.ext.fragment.ListReportTableColumns"
                        },
                        "ResponsiveTableCellsExtension|C_GenericSafetyInstructionTP": {
                            "type": "XML",
                            "className": "sap.ui.core.Fragment",
                            "fragmentName": "ehs.hs.si.managegnrcs1.ext.fragment.ListReportTableCells"
                        }
                    }
                },
                "sap.ui.controllerExtensions": {
                    "sap.suite.ui.generic.template.ListReport.view.ListReport": {
                        "controllerName": "ehs.hs.si.managegnrcs1.ext.controller.ListReportExt",
                        "sap.ui.generic.app": {
                            "C_GenericSafetyInstructionTP": {
                                "EntitySet": "C_GenericSafetyInstructionTP",
                                "Actions": {
                                    "Download": {
                                        "id": "idDownloadButton",
                                        "press": "onDownloadPress",
                                        "text": "{i18n>btnDownload}",
                                        "requiresSelection": true
                                    },
                                    "CreateFromPlus": {
                                        "id": "idCreateSIButton",
                                        "text": "{i18n>btnCreateToolbar}",
                                        "press": "openCreateSIDialog"
                                    }
                                }
                            }
                        }
                    },
                    "sap.suite.ui.generic.template.ObjectPage.view.Details": {
                        "controllerName": "ehs.hs.si.managegnrcs1.ext.controller.ObjectPageExt",
                        "sap.ui.generic.app": {
                            "C_GenericSafetyInstructionTP": {
                                "EntitySet": "C_GenericSafetyInstructionTP",
                                "Header": {
                                    "Actions": {
                                        "GenerateDocument": {
                                            "id": "idGenerateDocumentActionButton",
                                            "text": "{i18n>btnGenerate}",
                                            "press": "openGenerateDocumentDialog",
                                            "applicablePath": "objectPageExtModel>/bDisplayMode"
                                        },
                                        "CopyRevision": {
                                            "id": "idCopyRevisionButton",
                                            "text": "{i18n>btnCopyRevision}",
                                            "press": "onCopyRevisionPress",
                                            "applicablePath": "Copy_revision_ac"
                                        },
                                        "EditHeader": {
                                            "id": "idEditHeaderButton",
                                            "text": "{i18n>btnEditHeader}",
                                            "applicablePath": "ui>/editable",
                                            "press": "onEditHeaderPress"
                                        },
                                        "ReleaseRevision": {
                                            "id": "idReleaseRevisionButton",
                                            "text": "{i18n>btnRelease}",
                                            "press": "openReleaseRevisionDialog",
                                            "applicablePath": "Release_ac"
                                        },
                                        "Reject": {
                                            "id": "idRejectButton",
                                            "text": "{i18n>btnReject}",
                                            "press": "onRejectPress",
                                            "applicablePath": "Reject_ac"
                                        },
                                        "RequestRelease": {
                                            "id": "idRequestRelease",
                                            "text": "{i18n>btnRequestRelease}",
                                            "press": "onRequestReleasePress",
                                            "applicablePath": "Request_release_ac"
                                        },
                                        "SetToHistoric": {
                                            "id": "idSetToHistoric",
                                            "text": "{i18n>btnSetToHistoric}",
                                            "press": "onSetToHistoricPress",
                                            "applicablePath": "Set_to_historic_ac"
                                        },
                                        "EditNote": {
                                            "id": "idEditNote",
                                            "text": "{i18n>btnEditNote}",
                                            "applicablePath": "ui>/editable",
                                            "press": "onOpenEditInternalNoteDialog"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.ui.generic.app": {
        "_version": "1.3.0",
        "settings": {
            "forceGlobalRefresh": false,
            "objectPageHeaderType": "Dynamic",
            "showDraftToggle": false
        },
        "pages": {
            "ListReport|C_GenericSafetyInstructionTP": {
                "entitySet": "C_GenericSafetyInstructionTP",
                "component": {
                    "name": "sap.suite.ui.generic.template.ListReport",
                    "list": true,
                    "settings": {
                        "condensedTableLayout": true,
                        "smartVariantManagement": true,
                        "enableTableFilterInPageVariant": true
                    }
                },
                "pages": {
                    "ObjectPage|C_GenericSafetyInstructionTP": {
                        "entitySet": "C_GenericSafetyInstructionTP",
                        "component": {
                            "name": "sap.suite.ui.generic.template.ObjectPage"
                        }
                    }
                }
            }
        }
    },
    "sap.fiori": {
        "registrationIds": ["F4471"],
        "archeType": "transactional"
    },
    "sap.platform.hcp": {
        "uri": ""
    }
}
