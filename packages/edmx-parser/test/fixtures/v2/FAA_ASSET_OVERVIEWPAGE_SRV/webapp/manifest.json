                "settings": {
                    "localUri": "localService/FAA_ASSET_OVERVIEWPAGE_SRV/FAA_ASSET_OVERVIEWPAGE_ANNO_MDL.xml"
                }
            },
            "annotations": {
                "type": "ODataAnnotation",
