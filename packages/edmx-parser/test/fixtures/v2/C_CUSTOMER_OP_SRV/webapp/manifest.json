{
    "_version": "1.7.0",
    "sap.app": {
        "_version": "1.2.0",
        "id": "${project.artifactId}",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "${project.version}"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "tags": {
            "keywords": []
        },
        "ach": "LO-MD-BP",
        "resources": "resources.json",
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/C_CUSTOMER_OP_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": ["C_CUSTOMER_OP_ANNO_MDL", "localAnnotations"],
                    "localUri": "localService/metadata.xml"
                }
            },
            "C_CUSTOMER_OP_ANNO_MDL": {
                "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='C_CUSTOMER_OP_ANNO_MDL',Version='0001')/$value/",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "localService/C_CUSTOMER_OP_ANNO_MDL.xml"
                }
            },
            "localAnnotations": {
                "uri": "annotations/annotations.xml",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "annotations/annotations.xml"
                }
            }
        },
        "offline": false,
        "sourceTemplate": {
            "id": "ui5template.smarttemplate",
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "_version": "1.2.0",
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": ["sap_hcb", "sap_bluecrystal"]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "${sap.ui5.dist.version}",
            "libs": {
                "sap.ui.core": {
                    "lazy": false
                },
                "sap.m": {
                    "lazy": false
                },
                "sap.ui.comp": {
                    "lazy": false
                },
                "sap.uxap": {
                    "lazy": false
                },
                "sap.suite.ui.generic.template": {
                    "lazy": false
                },
                "sap.ui.generic.app": {
                    "lazy": false
                }
            },
            "components": {}
        },
        "models": {
            "i18n": {
                "preload": false,
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "@i18n": {
                "preload": false,
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ListReport|C_CustomerOP": {
                "preload": false,
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ListReport/C_CustomerOP/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ObjectPage|C_CustomerOP": {
                "preload": false,
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ObjectPage/C_CustomerOP/i18n.properties"
            },
            "": {
                "preload": true,
                "dataSource": "mainService",
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false
                }
            }
        },
        "extends": {
            "extensions": {}
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.ui.generic.app": {
        "_version": "1.1.0",
        "pages": [
            {
                "entitySet": "C_CustomerOP",
                "component": {
                    "name": "sap.suite.ui.generic.template.ListReport",
                    "list": true
                },
                "pages": [
                    {
                        "entitySet": "C_CustomerOP",
                        "component": {
                            "name": "sap.suite.ui.generic.template.ObjectPage",
                            "settings": {
                                "showRelatedApps": true
                            }
                        }
                    }
                ]
            }
        ]
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": ["F0046A"],
        "archeType": "transactional"
    },
    "sap.platform.hcp": {
        "_version": "1.2.0",
        "uri": ""
    }
}
