{
    "_version": "1.11.0",
    "sap.app": {
        "id": "${project.artifactId}",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "${project.version}"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "tags": {
            "keywords": []
        },
        "ach": "MM-PUR-PO",
        "crossNavigation": {
            "inbounds": {},
            "outbounds": {
                "MonitorMassChanges": {
                    "semanticObject": "PurchasingDocument",
                    "action": "showMassChangeResults"
                }
            }
        },
        "resources": "resources.json",
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/MM_PUR_POMASS_UPDATE_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": ["MM_PUR_POMASS_UPDATE_ANNO_MDL_01", "localAnnotations"],
                    "localUri": "localService/metadata.xml"
                }
            },
            "MM_PUR_POMASS_UPDATE_ANNO_MDL_01": {
                "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='MM_PUR_POMASS_UPDATE_ANNO_MDL_01',Version='0001')/$value/",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "localService/MM_PUR_POMASS_UPDATE_ANNO_MDL_01.xml"
                }
            },
            "localAnnotations": {
                "uri": "annotations/annotations.xml",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "annotations/annotations.xml"
                }
            }
        },
        "offline": false,
        "sourceTemplate": {
            "id": "ui5template.smartTemplate",
            "version": "1.40.12"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": ["sap_hcb", "sap_belize"]
    },
    "sap.ui5": {
        "flexEnabled": true,
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "${sap.ui5.dist.version}",
            "libs": {
                "sap.ui.generic.app": {},
                "sap.ui.core": {},
                "sap.m": {
                    "lazy": false
                },
                "sap.ui.comp": {
                    "lazy": false
                },
                "sap.uxap": {
                    "lazy": false
                },
                "sap.suite.ui.generic.template": {},
                "sap.ui.layout": {
                    "lazy": false
                },
                "sap.ushell": {
                    "lazy": false
                },
                "sap.collaboration": {
                    "lazy": false
                },
                "sap.ui.table": {
                    "lazy": false
                }
            },
            "components": {}
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ListReport|C_PurchaseOrderItemMassUpdate": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ListReport/C_PurchaseOrderItemMassUpdate/i18n.properties"
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "metadataUrlParams": {
                        "sap-value-list": "none"
                    },
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false
                }
            }
        },
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {
                    "sap.suite.ui.generic.template.ListReport.view.ListReport": {
                        "controllerName": "ui.s2p.mm.masspo.update.ext.controller.ListReportExt",
                        "sap.ui.generic.app": {
                            "C_PurchaseOrderItemMassUpdate": {
                                "EntitySet": "C_PurchaseOrderItemMassUpdate",
                                "Actions": {
                                    "ActionC_PurchaseOrderItemMassUpdate2": {
                                        "id": "ActionDeliveryScheduleLine",
                                        "text": "{@i18n>ActionDeliveryScheduleLine}",
                                        "press": "openDeliveryScheduleLines",
                                        "requiresSelection": true
                                    },
                                    "ActionC_PurchaseOrderItemMassUpdate1": {
                                        "id": "ItemMassEditAction",
                                        "text": "{@i18n>MassEditAction}",
                                        "press": "onMassEditButtonClick",
                                        "global": true
                                    },
                                    "ActionMonitorMassChanges": {
                                        "id": "ActionMonitorMassChanges",
                                        "text": "{@i18n>ActionMonitorMassChanges}",
                                        "press": "onClickMonitorMassChanges",
                                        "global": true
                                    }
                                }
                            }
                        }
                    }
                },
                "sap.ui.viewExtensions": {
                    "sap.suite.ui.generic.template.ListReport.view.ListReport": {
                        "ResponsiveTableCellsExtension|C_PurchaseOrderItemMassUpdate|_tab1": {
