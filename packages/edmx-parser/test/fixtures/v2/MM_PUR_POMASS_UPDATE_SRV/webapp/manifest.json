                            "type": "XML"
                        },
                        "ResponsiveTableColumnsExtension|C_PurchaseOrderItemMassUpdate|_tab1": {
                            "className": "sap.ui.core.Fragment",
                            "fragmentName": "ui.s2p.mm.masspo.update.ext.fragment.ListReportResponsiveTableColumn_Tab1",
                            "type": "XML"
                        },
                        "ResponsiveTableCellsExtension|C_PurchaseOrderItemMassUpdate|_tab2": {
                            "className": "sap.ui.core.Fragment",
                            "fragmentName": "ui.s2p.mm.masspo.update.ext.fragment.ListReportResponsiveTableCell_Tab2",
                            "type": "XML"
                        },
                        "ResponsiveTableColumnsExtension|C_PurchaseOrderItemMassUpdate|_tab2": {
                            "className": "sap.ui.core.Fragment",
                            "fragmentName": "ui.s2p.mm.masspo.update.ext.fragment.ListReportResponsiveTableColumn_Tab2",
                            "type": "XML"
                        },
                        "SmartFilterBarControlConfigurationExtension|C_PurchaseOrderItemMassUpdate": {
                            "className": "sap.ui.core.Fragment",
                            "fragmentName": "ui.s2p.mm.masspo.update.ext.fragment.Customfilter",
                            "type": "XML"
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.ui.generic.app": {
        "_version": "1.3.0",
        "settings": {},
        "pages": {
            "ListReport|C_PurchaseOrderItemMassUpdate": {
                "entitySet": "C_PurchaseOrderItemMassUpdate",
                "component": {
                    "name": "sap.suite.ui.generic.template.ListReport",
                    "list": true,
                    "settings": {
                        "smartVariantManagement": true,
                        "enableTableFilterInPageVariant": false,
                        "quickVariantSelectionX": {
                            "showCounts": true,
                            "variants": {
                                "0": {
                                    "key": "_tab1",
                                    "entitySet": "C_PurchaseOrderMassUpdate",
                                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionPresentationVariant#PurchaseOrders"
                                },
                                "1": {
                                    "key": "_tab2",
                                    "entitySet": "C_PurchaseOrderItemMassUpdate",
                                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionPresentationVariant#PurchaseOrderItems"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "sap.fiori": {
        "registrationIds": ["F2593"],
        "archeType": "transactional"
    },
    "sap.platform.hcp": {
